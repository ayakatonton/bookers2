<h2>User Info</h2>
<%= image_tag user.get_profile_image(100,100) %>
<table class="table">
  <tbody>
    <tr>
      <th>name</th>
      <td><%= user.name %></td>
    </tr>
    <tr>
      <th>introduction</th>
      <td><%= user.introduction %></td>
    </tr>
  </tbody>
</table>
<%= link_to edit_user_path(user) , class:"nav-link btn btn-outline-secondary btn-sm" do %>
  <i class="fa-solid fa-user-gear"></i>
<% end %>
   